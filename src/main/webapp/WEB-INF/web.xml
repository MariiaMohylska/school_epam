<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>ShowClassServlet</servlet-name>
    <servlet-class>controller.ShowClassServet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>StudentInClassServlet</servlet-name>
    <servlet-class>controller.StudentInClassServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ShowClassServlet</servlet-name>
    <url-pattern>/main</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StudentInClassServlet</servlet-name>
    <url-pattern>/students</url-pattern>
  </servlet-mapping>
</web-app>
